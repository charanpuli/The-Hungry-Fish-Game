<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.charanpuli.fish_baller">

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_icon2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash_icon2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityGameOver" />
        <activity android:name=".StartActivity" />
        <activity android:name=".LeaderActivity" />
        <activity android:name=".TellUrFriendsActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LoginLeaderActivity" />
        <activity android:name=".NoticeActivity"></activity>
    </application>

</manifest>